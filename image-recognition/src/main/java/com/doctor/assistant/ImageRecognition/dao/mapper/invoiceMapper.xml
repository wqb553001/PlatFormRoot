<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.doctor.assistant.ImageRecognition.dao.ElementDao">
    <insert id="batchInsertElementList" useGeneratedKeys="true">
        insert into Element(commodityId, word, row)
        values
        <foreach item="item" index="index" collection="elementList" separator=",">
            (#{item.commodityId},#{item.word},#{item.row})
        </foreach>
    </insert>
</mapper>
